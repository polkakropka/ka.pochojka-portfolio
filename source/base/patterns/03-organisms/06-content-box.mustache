<div class="content-box">
    <div class="content-box__container">
        {{#container1}}
            <div class="content-box__headline should-know">
                <h3><span class="big">7</span> Things you <span class="grey">should know </span>about me</h3>
            </div>
            <div class="content-box__content">
                {{#ifList}}
                   {{> molecules-ul}}
                {{/ifList}}
            </div>
        {{/container1}}
    </div>
    <div class="content-box__container">
        {{#container2}}
            <div class="content-box__headline dont-care">
                <h3><span class="big">7</span> Things you <span class="grey">don't care </span>about</h3>
            </div>
            <div class="content-box__content">
                {{#ifList}}
                    {{> molecules-ul}}
                {{/ifList}}
            </div>
        {{/container2}}
    </div>
</div>